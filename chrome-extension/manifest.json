{
  "manifest_version": 3,
  "name": "Insta Image Logger",
  "version": "1.0",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["http://127.0.0.1:5000/"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
